{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["es2021", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {}
  },
  "exclude": ["node_modules", "dist", ".next"]
}
